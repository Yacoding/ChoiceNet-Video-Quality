python webcam.py 127.0.0.1 1 2 --server 1 --bind_address 127.0.0.1 --quality 1000000
python webcam.py 127.0.0.1 1 2 --server 1 --bind_address 127.0.0.1 --quality 1000000000
python webcam.py 127.0.0.1 1 2 --server 1 --bind_address 127.0.0.1 --quality 1
